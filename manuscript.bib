journal = {PLoS Computational Biology},
year = {2012},
volume = {8},
number = {5},
pages = {e1002514},
publisher = {Public Library of Science},
affiliation = {ETH Zurich, Department of Computer Science, Z{\"u}rich, Switzerland.},
doi = {10.1371/journal.pcbi.1002514},

number = {1},
pages = {15--24},
publisher = { Mary Ann Liebert, Inc.},
affiliation = {Department of Molecular Genetics, Weizmann Institute of Science, Rehovot, Israel and Bioinformatics Unit, Department of Biological Services, Weizmann Institute of Science, Rehovot, Israel. itai.yanai@weizmann.ac.il},
doi = {10.1089/153623104773547462},
